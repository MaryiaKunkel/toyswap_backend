{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://our.company.com/listingUpdate.schema.json",
  "type": "object",
  "properties": {
    "title": {
      "type": "string",
      "minLength": 1,
      "maxLength": 50
    },
    "description": {
      "type": "string",
      "minLength": 1
    },
    "image_url": {
      "type": "string",
      "format": "uri"
    },
    "available": {
      "type": "boolean"
    },
    "state": {
      "type": "string",
      "minLength": 1
    },
    "city": {
      "type": "string",
      "minLength": 1
    }
  },
  "additionalProperties": false,
  "required": ["title"]
}
